{
  "version": 2,
  "builds": [
    {
      "src": "Backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/book/(.*)",
      "dest": "Backend/index.js"
    },
    {
      "src": "/user/(.*)",
      "dest": "Backend/index.js"
    },
    {
      "src": "/contact/(.*)",
      "dest": "Backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "Frontend/index.html"
    }
  ],
  "env": {
    "PORT": "3005",
    "MongogDBURI": "${MongogDBURI}"
  }
}